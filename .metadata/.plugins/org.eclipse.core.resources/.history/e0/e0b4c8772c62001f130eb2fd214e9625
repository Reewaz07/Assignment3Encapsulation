package Inheritance;


class A{
	private void print() {
		System.out.println("Class A(This) is parent Class");
	}
}	
class B extends A{
	void print(){
		System.out.println("(ClassB)This is child Class:");
		super.print();
	}
}

	

public class Question2 extends B {
	public static void main(String[] args) {
		A obj=new A();
		obj.print();
		B obj1= new B();
		obj1.print();
	}
	

}
